module demos::ns;

struct Foo {
  value: int32;
}

struct Bar {
  value: int32;
}

fn label(foo: Foo) -> string {
  "foo"
}

fn label(bar: Bar) -> string {
  "bar"
}

fn main() {
  let Foo a = Foo{
    1,
  };

  let Bar b = Bar{
    2,
  };

  let string foo_free = label(a);
  let string bar_free = label(b);
  out(`Foo via free function: {foo_free}`);
  out(`Bar via free function: {bar_free}`);
  out(`Foo method label: {foo_free}`);
  out(`Bar method label: {bar_free}`);
}
