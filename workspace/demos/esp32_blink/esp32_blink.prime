module demos::esp32_blink;

fn main() {
  let int32 my_var = 5;
  out(`hello from esp32 blink demo and value is {my_var}`);

  let int32 led = 2;
  pin_mode(led, 1);
  out(`This is our second text {my_var}`);

  let bool state = true;
  out(`State is: {state}`);
  loop {
    digital_write(led, 0);
    delay_ms(1000);
    digital_write(led, 1);
    delay_ms(1000);
  }
}
