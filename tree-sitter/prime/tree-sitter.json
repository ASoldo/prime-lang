{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "prime",
      "camelcase": "Prime",
      "title": "Prime",
      "scope": "source.prime",
      "file-types": [
        "prime"
      ],
      "injection-regex": "^prime$",
      "class-name": "TreeSitterPrime"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "PrimeLang",
    "authors": [
      {
        "name": "Andrej Soldo",
        "email": "soldo.andrej@gmail.com",
        "url": "https://rootster.xyz/"
      }
    ],
    "links": {
      "repository": "https://github.com/ASoldo/prime_lang_treesitter"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
