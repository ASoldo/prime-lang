struct Vec2 {
  x: float32;
  y: float32;
}

struct Transform {
  position: Vec2;
  velocity: Vec2;
}

struct Stats {
  hp: int32;
  stamina: int32;
}

struct Player {
  Transform;
  Stats;
  name: string;
  level: int32;
}

struct Enemy {
  Transform;
  kind: string;
  attack: int32;
}

struct Quest {
  title: string;
  reward: int32;
  completed: bool;
}
